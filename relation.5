#!/bin/bash


a=20
b=30

#Relatinal Operator 

if [ $a -eq $b ]
then
    echo "a is equal to b"
fi

if [ $a -ne $b ]
then
    echo "a is not equal to b"
fi

if [ $a -gt $b ] 
then
    echo "a is Greater than b"
fi

if [ $a -lt $b ]
then 
    echo "a is less than b"
fi

if [ $a -ge $b ]
then
    echo "a is greate than equal to b"
fi

if [ $a -le $b ] 
then
    echo "a is less than equal to b"
fi

 
