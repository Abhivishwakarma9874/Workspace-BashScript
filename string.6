#!/bin/bash

a="abc"
b="efg"

if [ $a = $b ]
then
	echo "$a =$b : a is equal to b"
else
	echo "$a = $b : a is not equal to b"
fi

if [ $a != $b ]
then
	echo "$a != $b : a is not equal to b"
else 
	echo "$a != $b : a is equal to b"
fi

if [ -z $a ]
then
	echo "-z $a : String length is Zero"
else
	echo "-z $a : String length is not Zero"
fi
